<!--
*******************************
	RT-Druid DIRECTORIES
*******************************
-->
	<property name="rtd_stdalone_dir"        value="rtd_stdalone_${rtd_core_v}"/>
	<property name="rtd_stdalone_helper_dir" value="com.eu.evidence.rtdruid.stdalone.helper_${rtd_core_v}"/>

