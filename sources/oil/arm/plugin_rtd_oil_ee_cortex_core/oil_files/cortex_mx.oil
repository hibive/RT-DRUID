OIL_VERSION = "2.4";

IMPLEMENTATION ee {
    OS {
        ENUM [
            CORTEX_MX {
                STRING ID = "default_cpu";
                STRING APP_SRC[];
                BOOLEAN [
                    TRUE {
                        BOOLEAN [
                            TRUE {
                                UINT32 SYS_SIZE;
                            },
                            FALSE
                        ] IRQ_STACK;
                    },
                    FALSE
                ] MULTI_STACK = FALSE;
                
                BOOLEAN JLINK = FALSE;
                ENUM [
                	IAR,
                	CCS,
                	KEIL
                ] COMPILER_TYPE;
                
                ENUM [
                	M0,
                	// M1,
                	// M2,
                	M4
                ] MODEL; 
                FLOAT CPU_CLOCK;                
            }
            
        ] CPU_DATA[];

        ENUM [
        	LPCXPRESSO {
                ENUM [
                    CUSTOM {
                        STRING MODEL;
						STRING LINKERSCRIPT;
						STRING INCLUDE_C;
						STRING INCLUDE_S;
						STRING STARTUP;
                    },
                    LPC12xx
             	] MODEL;
       		},
       		STELLARIS {
                ENUM [
                    CUSTOM {
                        STRING MODEL;
						STRING LINKERSCRIPT;
						STRING INCLUDE_C;
						STRING INCLUDE_S;
						STRING STARTUP;
                    },
                    LM4F232xxxx
             	] MODEL;
       		}
       		
     	] MCU_DATA[];
   };
};
