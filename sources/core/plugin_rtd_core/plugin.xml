         <description>%cheatsheet.sample.desc</description>
      </cheatsheet>

   </extension>
-->   

<!--
	Object Properties Factories
-->

   <extension-point
   		id="objPropertiesFactoryID"
   		name="required Object Properties Factory"
   		schema="schema/com.eu.evidence.rtdruid.core.objPropertiesFactoryID.exsd"/>
   <extension-point id="data_tranformation" name="RT-Druid data importer and exporter" schema="schema/data_tranformation.exsd"/>
<!--
   <extension
         point="com.eu.evidence.rtdruid.core.objPropertiesFactoryID">
      <objPropertiesFactory
            class="com.eu.evidence.rtdruid.internal.vartree.data.test.ObjPropertiesFactoryTest"/>
   </extension>
-->
	<extension
         point="com.eu.evidence.rtdruid.core.objPropertiesFactoryID">
      <objPropertiesFactory
            class="com.eu.evidence.rtdruid.factory.ObjPropertiesFactoryTest"/>
   </extension>
 <extension
       point="com.eu.evidence.rtdruid.core.data_tranformation">
    <exporter
          case_sensitive="false"
          class="com.eu.evidence.rtdruid.io.RTDXsltImportExport"
          type="rtd">
    </exporter>
    <importer
          case_sensitive="false"
          class="com.eu.evidence.rtdruid.io.RTDXsltImportExport"
          type="rtd">
    </importer>
 </extension>

</plugin>
