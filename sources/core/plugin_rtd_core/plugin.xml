<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.3"?>
<plugin>

<!--
	Extentions to CVS
-->
  <extension point="org.eclipse.team.core.fileTypes">
	<fileTypes extension="rtd" type="text"/>
	<fileTypes extension="ertd" type="text"/>
  </extension>

<!--
	Extentions to File types
-->
  <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser type="rtd"  class="com.eu.evidence.rtdruid.vartree.data.init.RTD_XMI_Factory"/>
      <parser type="rtD"  class="com.eu.evidence.rtdruid.vartree.data.init.RTD_XMI_Factory"/>
      <parser type="rTd"  class="com.eu.evidence.rtdruid.vartree.data.init.RTD_XMI_Factory"/>
      <parser type="Rtd"  class="com.eu.evidence.rtdruid.vartree.data.init.RTD_XMI_Factory"/>
      <parser type="rTD"  class="com.eu.evidence.rtdruid.vartree.data.init.RTD_XMI_Factory"/>
      <parser type="RtD"  class="com.eu.evidence.rtdruid.vartree.data.init.RTD_XMI_Factory"/>
      <parser type="RTd"  class="com.eu.evidence.rtdruid.vartree.data.init.RTD_XMI_Factory"/>
      <parser type="RTD"  class="com.eu.evidence.rtdruid.vartree.data.init.RTD_XMI_Factory"/>
      
      <parser type="ertd" class="com.eu.evidence.rtdruid.vartree.data.init.RTD_XMI_Factory"/>
  </extension>


<!--
	
-->
   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.evidence.eu.com/rtdruid/data"
            class="com.eu.evidence.rtdruid.vartree.data.DataPackage"/>
   </extension>

<!--
	EMF Factories
-->

   <extension-point
   		id="requiredAdapterFactoriesID"
   		name="required Adapter Factories"
   		schema="schema/com.eu.evidence.rtdruid.core.requiredAdapterFactoriesID.exsd"/>

   <extension
         point="com.eu.evidence.rtdruid.core.requiredAdapterFactoriesID">
      <loader
            class="com.eu.evidence.rtdruid.vartree.data.init.AdapterFactoryLoaderImpl"/>
   </extension>


   <!-- Tasks - ->
   <extension
         point="org.eclipse.ant.core.antTasks">
      <antTask
            library="ant_tasks/rtdruid_ant_task.jar"
            name="rtdruid.Convert"
            class="com.eu.evidence.rtdruid.vartree.data.ant.Convert"/>
   </extension>-->
   <!-- Extra Classpath -->
   <extension
         point="org.eclipse.ant.core.extraClasspathEntries">
      <extraClasspathEntry library="rtdruid_core_ant.jar"/>
   </extension>
   
   <!-- Cheat Sheet -->
